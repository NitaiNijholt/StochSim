   "source": [
    "def run_convergence_simulations(grid, xmin, xmax, ymin, ymax, threshold, iteration_range, sample_range, num_runs, seeds = None):\n",
    "    '''Runs multiple stochastic simulations of the Mandelbrot set area estimation\n",
    "    using predefined ranges of recursion iterations and sample sizes.\n",
    "    If passed a list of seed values, each simulation is run with one of the seeds from the list,\n",
    "    otherwise no seed is used.\n",
    "    Returns a DataFrame containing per entry: [Number of iterations, Number of samples, Simulation index, Area estimate].\n",
    "    '''\n",
    "    if seeds.any() and seeds.shape[0] == num_runs:\n",
    "        use_seed = True\n",
    "    else: use_seed = False\n",
    "    area_table = np.empty((len(iteration_range)*len(sample_range)*num_runs, 4))\n",
    "    for i, iteration in enumerate(iteration_range):\n",
    "        for j, sample_size in enumerate(sample_range):\n",
    "            for k in range(num_runs):\n",
    "                # Assign seed if provided\n",
    "                if use_seed:\n",
    "                    seed = seeds[k]\n",
    "                else:\n",
    "                    seed = None\n",
    "                area, _ = calculate_area_random_grid_sampling(grid, xmin, xmax, ymin, ymax, threshold, iteration, sample_size, seed=seed)\n",
    "                area_table[(i*len(sample_range)+j)*num_runs+k] = np.array([iteration, sample_size, k, area])\n",
    "    df_area = pd.DataFrame(area_table, columns=['Iterations', 'Samples', 'Simulation', 'Area'])\n",
    "    return df_area"
   ]
  },
