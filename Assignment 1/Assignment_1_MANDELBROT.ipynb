    "def calculate_area_random_grid_sampling(grid, xmin, xmax, ymin, ymax, threshold, iterations, sample_size, seed=None):\n",
    "    '''Estimates the area of the Mandelbrot set within a predefined region\n",
    "    using a randomly sampled grid.\n",
    "    sample_size is a value between 0 and 1 corresponding to a fraction of the grid size.\n",
    "    '''\n",
    "    if seed is not None:\n",
    "        np.random.seed(seed)\n",
    "    \n",
    "    points = np.random.choice(np.reshape(grid, np.prod(grid.shape)), size=int(sample_size*np.prod(grid.shape)), replace=False)\n",
    "\n",
    "    output = vectorized_mandlebrot(points, iterations, threshold)\n",
    "    \n",
    "    max_val = np.max(output)\n",
    "    area_ratio = np.sum(output == max_val) / sample_size\n",
    "    total_area = (xmax - xmin) * (ymax - ymin)\n",
    "    \n",
    "    return area_ratio * total_area, output.shape"
   ]
  },
